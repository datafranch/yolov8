    "computeConfigs": [
      {
        "name": "yolov8-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "ultralytics/ultralytics:8.0.183",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          },
          "preemptible": false
        }
      },
      {
        "name": "yolov8-train-evaluate",
        "driverId": "",
